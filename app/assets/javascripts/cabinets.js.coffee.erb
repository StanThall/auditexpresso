$(document).ready ->
 $('i.btn_classement_up').click (event) ->
  event.preventDefault()
  id = $(this).attr('id')
  $('i.btn_classement_up').fadeOut()
 		
  $.ajax
   type: 'put'
   url: '/cabinets/up/'+id
   dataType: 'json'
   success: (json) ->
    $('td#rating_' + id ).text(json.rating).effect('highlight')